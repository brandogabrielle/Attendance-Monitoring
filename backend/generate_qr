import qrcode
import json

#studentinfo
student = {
    "student_id": "22-17-046",
    "last_name": "Paa√±o",
    "first_name": "Brando Gabrielle"
}


#convert student info to JSON
qr_data = json.dumps(student)

#generate QR code
qr = qrcode.make(qr_data)

#save QR code as an image file
qr.save("student_info.png")
print("QR code generated and saved as student_info.png")
